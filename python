import numpy as np

a=eval(input('Enter the data in [x,y] form: '))
y=np.array([a[1]])
x=np.array([a[0]])
x=x.flatten()
y=y.flatten()
if len(x)>=11:
    l=10
else:
    l=len(x)-1
n=np.zeros((l,1))
print(n)
for i in range (1,l):
    f=np.polyfit(x,y,i)
    v=np.polyval(f,x)
    n[i-1,0]=np.linalg.norm(y-v)
    I=n.argmin()
    c=np.polyfit(x,y,I+1)
    print('These are the coefficients: \n',c)
